                        </table:template>
                    </table:Column>
                    <table:Column label="">
                        <table:label>
                            <Label text="Fiili 01 Oran" design="Bold" wrapping="true"></Label>
                        </table:label>
                        <table:template>
                            <ObjectStatus text="{jsonModel>FiiliOran01}" state="None"></ObjectStatus>
                        </table:template>
                    </table:Column>
                    <table:Column label="">
                        <table:label>
                            <Label text="Fiili 02" design="Bold" wrapping="true"></Label>
                        </table:label>
                        <table:template>
                            <ObjectStatus text="{jsonModel>Fiili02}" state="Error"></ObjectStatus>
                        </table:template>
                    </table:Column>
                    <table:Column label="">
                        <table:label>
                            <Label text="Fiili 03" design="Bold" wrapping="true"></Label>
                        </table:label>
                        <table:template>
                            <ObjectStatus text="{jsonModel>Fiili03}" state="Information"></ObjectStatus>
                        </table:template>
                    </table:Column>
                    <table:Column label="">
