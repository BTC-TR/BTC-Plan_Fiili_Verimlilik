                        </table:label>
                        <table:template>
                            <ObjectStatus text="{jsonModel>Fiili04}" state="Warning"></ObjectStatus>
                        </table:template>
                    </table:Column>
                    <table:Column label="">
                        <table:label>
                            <Label text="Fiili Toplam" design="Bold" wrapping="true"></Label>
                        </table:label>
                        <table:template>
                            <ObjectStatus text="{jsonModel>FiiliToplam}" state="Success"></ObjectStatus>
                        </table:template>
                    </table:Column>
                    <table:Column label="">
                        <table:label>
                            <Label text="Modül Verimlilik" design="Bold" wrapping="true"></Label>
                        </table:label>
                        <table:template>
                            <ObjectStatus text="{jsonModel>ModulVerimlilik}" state="None"></ObjectStatus>
                        </table:template>
                    </table:Column>
                    <table:Column label="">
                        <table:label>
                            <Label text="Sözleşme Planlanan Gün" design="Bold" wrapping="true"></Label>
                        </table:label>
                        <table:template>
                            <ObjectStatus text="{jsonModel>PlanSozlesmeGun}" state="Error"></ObjectStatus>
                        </table:template>
                    </table:Column>
                    <table:Column label="">
                        <table:label>
                            <Label text="Planlanan Ek CR Gün" design="Bold" wrapping="true"></Label>
                        </table:label>
                        <table:template>
                            <ObjectStatus text="{jsonModel>PlanEkcrGun}" state="Information"></ObjectStatus>
                        </table:template>
                    </table:Column>
                    <table:Column label="">
                        <table:label>
                            <Label text="Planlanan Toplam Gün" design="Bold" wrapping="true"></Label>
                        </table:label>
                        <table:template>
                            <ObjectStatus text="{jsonModel>PlanToplamGun}" state="Warning"></ObjectStatus>
                        </table:template>
                    </table:Column>
                    <table:Column label="">
                        <table:label>
                            <Label text="Fiili 01 Gün" design="Bold" wrapping="true"></Label>
                        </table:label>
                        <table:template>
                            <ObjectStatus text="{jsonModel>Fiili01Gun}" state="Success"></ObjectStatus>
                        </table:template>
                    </table:Column>
                    <table:Column label="">
                        <table:label>
                            <Label text="Kalan Gün" design="Bold" wrapping="true"></Label>
                        </table:label>
                        <table:template>
                            <ObjectStatus text="{jsonModel>KalanGun}" state="None"></ObjectStatus>
                        </table:template>
                    </table:Column>
                </table:columns>
            </table:Table>
